{
  "expo": {
    "name": "Multi-N-Back",
    "slug": "n-back-trainer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/brain.png",
    "scheme": "nbackshift",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "icon": "./assets/images/brain.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.todoaniki.n-back-trainer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/brain.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.todoaniki.nbacktrainer"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/brain.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "afc1b58b-6b90-44d6-a212-f93e6801e121"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/afc1b58b-6b90-44d6-a212-f93e6801e121"
    }
  }
}
